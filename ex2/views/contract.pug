doctype html
html
  head
    title= title
    link(rel='stylesheet', href='/stylesheets/w3.css')
  body
    .w3-card-4
    header.w3-container.w3-teal
        h1 #[a.w3-btn.w3-green(href="/") Home ] Contracts List
    
    .w3-container
        table.w3-table-all
            tr 
                th Nome da Instituição 
                td #{contract.NomeInstituicao}
            tr 
                th NIPC da Instituição 
                td #{contract.NIPCInstituicao}
            tr 
                th Nome do Titular do Contrato 
                td #{contract.NomeTitularContrato}
            tr 
                th ID da Ciência 
                td #{contract.CienciaID}
            tr 
                th ORCID 
                td #{contract.ORCID}
            tr 
                th Ciencia Vitae 
                td #{contract.CienciaVitae}
            tr 
                th Carreira RPN 
                td #{contract.Carreira_RPN}
            tr 
                th Categoria RPN 
                td #{contract.Categoria_RPN}
            tr 
                th Vínculo RPN 
                td #{contract.Vinculo_RPN}
            tr 
                th Regime de Prestação de Servico 
                td #{contract.RegimePrestacaoServico}
            tr 
                th Data do Início de Contrato 
                td #{contract.DataInicioContrato}
            tr 
                th Data do Fim de Contrato 
                td #{contract.DataFimContrato}
            tr 
                th ETI remunerado 
                td #{contract.ETIremunerado}
            tr 
                th Procedimento Vinculação 
                td #{contract.ProcedimentoVinculacao}
            tr 
                th Áreas de Investigação
                td #{contract.AreasInvestigacao}
            tr 
                th Nivel de Formação 
                td #{contract.NivelFormação}
            tr 
                th Instituição de Ensino 
                td #{contract.InstituicaoEnsino}
            tr 
                th País de Instituição 
                td #{contract.PaisInstituição}
            tr 
                th Ano do Diploma 
                td #{contract.AnoDiploma}
            tr 
                th Curso 
                td #{contract.Curso}
            tr 
                th Area CNAEF 
                td #{contract.AreaCNAEF}
            tr 
                th Area FORD 
                td #{contract.AreaFORD}
            tr 
                th Provas de Agregação
                td #{contract.ProvasAgregacao}
            tr 
                th Título de Especialista 
                td #{contract.TituloEspecialista}
            tr 
                th Provas de Coordenação
                td #{contract.ProvasCoordenacao}
            tr 
                th Provas de Aptidão
                td #{contract.ProvasAptidao}

    footer.w3-container.w3-teal
      h5 Generated by RPCW2023 in #{d}